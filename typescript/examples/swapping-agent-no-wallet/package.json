{
  "name": "swapping-agent-no-wallet",
  "version": "1.0.0",
  "description": "Ember On-chain Actions swapping Agent Example without integrated wallet",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build && cpx \"encyclopedia/**/*\" dist/encyclopedia",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "pnpm --filter arbitrum-vibekit build && pnpm --filter ember-api build && pnpm --filter test-utils build && NODE_OPTIONS=\"--no-warnings\" tsx node_modules/mocha/bin/mocha --exit --timeout 120000 --config ../../.mocharc.json 'test/**/*.test.ts'",
    "inspect:npx": "pnpm run build && npx -y @modelcontextprotocol/inspector node ./dist/index.js",
    "docker:build": "docker build -t swapping-agent-no-wallet ../../",
    "docker:run": "docker run --env-file .env -p 3001:3001 swapping-agent-no-wallet",
    "docker:compose:up": "docker compose up --build --no-log-prefix",
    "docker:compose:down": "docker compose down",
    "docker:compose:build": "docker compose build --no-cache",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@eslint/js": "^9.0.0",
    "@google-a2a/types": "github:google-a2a/A2A#path:types",
    "@modelcontextprotocol/sdk": "^1.13.1",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@openzeppelin/contracts": "^5.3.0",
    "ai": "^4.3.2",
    "arbitrum-vibekit-core": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ember-api": "workspace:*",
    "eslint": "^9.0.0",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "fastify": "^5.1.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "viem": "^2.19.8",
    "ws": "^8.18.2",
    "zod": "^3.25.7"
  },
  "devDependencies": {
    "@types/chai": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.0",
    "chai": "^5.1.2",
    "cpx": "^1.5.0",
    "mocha": "^10.8.2",
    "mocha-suppress-logs": "^0.3.1",
    "prettier": "^3.5.3",
    "test-utils": "workspace:*",
    "tsx": "^4.6.2"
  }
}
