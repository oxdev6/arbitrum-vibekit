{
  "name": "swapping-agent",
  "version": "1.0.0",
  "description": "Ember On-chain Actions swapping Agent Example",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "inspect:npx": "pnpm run build && npx -y @modelcontextprotocol/inspector node ./dist/index.js",
    "docker:build": "docker build -t swapping-agent .",
    "docker:run": "docker run --env-file .env -p 3001:3001 swapping-agent",
    "docker:compose:up": "docker compose up --build",
    "docker:compose:down": "docker compose down",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@emberai/sdk-typescript": "0.3.0-beta.6",
    "@grpc/grpc-js": "^1.12.5",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "arbitrum-vibekit": "workspace:*",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "ai": "^4.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ember-mcp-tool-server": "workspace:*",
    "express": "^4.21.2",
    "tsx": "^4.6.2",
    "viem": "^2.26.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.5.3",
    "typescript": "^5.3.2"
  }
}
