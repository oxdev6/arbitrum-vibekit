{
  "name": "pendle-agent-example",
  "version": "1.0.0",
  "description": "Ember On-chain Actions Pendle Agent Example",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
    "dev": "tsx src/index.ts",
    "start": "pnpm run build && node dist/index.js",
    "client": "tsx src/mcp-client.ts",
    "inspect:npx": "pnpm run build && npx -y @modelcontextprotocol/inspector node ./dist/index.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@emberai/sdk-typescript": "file:../../../../ember-sdk-typescript",
    "@grpc/grpc-js": "^1.12.5",
    "@modelcontextprotocol/sdk": "^1.10.0",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "ai": "^4.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "express": "^4.21.2",
    "tsx": "^4.6.2",
    "viem": "^2.26.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.5.3",
    "typescript": "^5.3.2"
  }
}
